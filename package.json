{
  "name": "mz-vue3-admin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build-github": "vue-tsc --noEmit && vite build --base=/mz-vue3-admin/ --mode github-io",
    "deploy": "deploy.sh",
    "preview": "vite preview",
    "lint": "npm run lint:eslint & npm run lint:stylelint & npm run lint:prettier",
    "lint:eslint": "eslint --fix ./src/**/*.{js,jsx,ts,tsx,vue} --cache --cache-location node_modules/.cache/eslint/",
    "lint:stylelint": "stylelint --fix ./src/**/*.{css,scss,vue} --cache --cache-location node_modules/.cache/stylelint/",
    "lint:prettier": "prettier --write ./src/**/*.{js,ts,json,tsx,css,less,scss,vue,html,md} --loglevel warn --cache",
    "lint:lint-staged": "lint-staged",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vitejs/plugin-vue-jsx": "^2.1.1",
    "@vueuse/core": "^9.6.0",
    "axios": "^1.2.0",
    "echarts": "^5.4.0",
    "element-plus": "^2.2.20",
    "lodash-es": "^4.17.21",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.23",
    "pinia-plugin-persistedstate": "^2.4.0",
    "three": "^0.150.1",
    "vue": "^3.2.41",
    "vue-router": "4",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@iconify-json/bi": "^1.1.15",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.9",
    "@types/nprogress": "^0.2.0",
    "@types/three": "^0.149.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.7.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.20",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.7.1",
    "sass": "^1.56.1",
    "stylelint": "^14.16.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.3.0",
    "typescript": "^4.6.4",
    "unocss": "^0.46.5",
    "unplugin-auto-import": "^0.11.4",
    "unplugin-vue-components": "^0.22.9",
    "vite": "^3.2.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vue-tsc": "^1.0.9"
  }
}